{
  "routes": [
    {
      "route": "/api/getSampleList",
      "allowedRoles": ["anonymous"],
      "serve": "https://taxcredit-ai-func-v2.azurewebsites.net/api/getSampleList",
      "methods": ["GET", "OPTIONS"],
      "priority": 100
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200,
      "priority": 300
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/static/*",
      "/assets/*",
      "/*.{css,js,png,jpg,gif,ico,svg,json}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self' https://taxcredit-ai-func-v2.azurewebsites.net; connect-src 'self' https://taxcredit-ai-func-v2.azurewebsites.net data: blob: *; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self' data:;",
    "X-Content-Type-Options": "nosniff"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".html": "text/html",
    ".js": "text/javascript"
  },
  "platform": {
    "apiRuntime": "node:16"
  }
}
