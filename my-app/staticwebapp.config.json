{
  "routes": [
    {
      "route": "/api/getSampleList*",
      "redirect": "https://taxcredit-api-func-v2.azurewebsites.net/api/getSampleList"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/static/*",
      "/assets/*",
      "/*.{css,js,png,jpg,gif,ico,svg,json}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
