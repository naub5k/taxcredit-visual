{
  "routes": [
    {
      "route": "/api/getSampleList",
      "allowedRoles": ["anonymous"],
      "serve": "https://taxcredit-api-func-v2.azurewebsites.net/api/getSampleList",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/static/*",
      "/assets/*",
      "/*.{css,js,png,jpg,gif,ico,svg,json}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self' https://taxcredit-api-func-v2.azurewebsites.net; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self' data:;",
    "X-Content-Type-Options": "nosniff"
  }
}
