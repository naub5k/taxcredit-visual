{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/images/*", "/*.{png,jpg,gif,ico,css,js}"]
  },
  "routes": [
    {
      "route": "/health",
      "serve": "/health.html"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
} 