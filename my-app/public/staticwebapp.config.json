{
  "routes": [
    {
      "route": "/api/getSampleList",
      "rewrite": "https://taxcredit-api-func-v2.azurewebsites.net/api/getSampleList",
      "methods": ["GET", "OPTIONS"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/getSampleList/*",
      "rewrite": "https://taxcredit-api-func-v2.azurewebsites.net/api/getSampleList/{rest}",
      "methods": ["GET", "OPTIONS"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/data-api/*", "/images/*", "/*.ico", "/*.png", "/*.svg"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
