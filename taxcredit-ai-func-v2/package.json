{
  "name": "taxcredit-ai-func-v2",
  "version": "1.0.0",
  "description": "",
  "main": "src/aimodelquery/index.js",
  "scripts": {
    "start": "func start",
    "start:alt": "func start --port 7072",
    "test": "node test.js",
    "test:azure": "node test.js --azure",
    "test:azure:detail": "node detailed-azure-test.js",
    "test:simple": "node simple-test.js",
    "test:keys": "node key-test.js",
    "test:with-env": "set-env-and-test.bat",
